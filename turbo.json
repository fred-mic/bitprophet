{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": ["bun.lock"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".bun", "dist/**"],
      "cache": true
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}


